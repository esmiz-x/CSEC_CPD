#include<bits/stdc++.h>
using namespace std;
int main(){
    string s;
    cin>>s;
    set<string>st;
    for(int i=0; i<s.size(); i++){
        st.insert(s.substr(i,1));
    }
    if(st.size()%2 != 0)
    {
        cout<<"IGNORE HIM!"<<endl;
    }
    else{
        cout<<"CHAT WITH HER!"<<endl;
    }
}
