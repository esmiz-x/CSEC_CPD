#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    string a,d;
    for(int i=0; i<n; i++){
        string s(n,' ');
        cin>>s[i];
        if(s[i]=='A'){
            a+=s[i];
        }
        else{
            d+=s[i];
        }
    }
    if(a.size()>d.size()){
        cout<<"Anton\n";
    }
    else if(a.size()<d.size()){
        cout<<"Danik\n";
    }
    else{
        cout<<"Friendship\n";
    }
}
